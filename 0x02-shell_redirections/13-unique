#!/bin/sh

# Read input from command line
read -p "Enter a list of words: " input

# Split input into an array of words
IFS=$'\n' read -r -a words <<< "$input"

# Create an associative array to count word occurrences
declare -A count

# Count word occurrences
for word in "${words[@]}"; do
    count["$word"]=$((count["$word"] + 1))
done

# Sort words alphabetically
sorted_words=$(printf "%s\n" "${words[@]}" | sort)

# Print words that appear exactly once
prev_word=""
for word in $sorted_words; do
    if [ "$word" != "$prev_word" ]; then
        if [ "${count["$word"]}" -eq 1 ]; then
            echo "$word"
        fi
    fi
    prev_word="$word"
done

